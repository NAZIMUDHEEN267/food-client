{
  "expo": {
    "name": "Food delivery",
    "slug": "food_delivery",
    "version": "0.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": {
        "dark": "./assets/images/icons/ios-dark.png",
        "light": "./assets/images/icons/ios-light.png"
      },
      "bundleIdentifier": "com.nazimudheen.foodDelivery"
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icons/profile.png",
        "backgroundColor": "#ed7c26"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.nazimudheen.food_delivery"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icons/profile.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFA920"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-quick-actions",
        {
          "androidIcons": {
            "profile": {
              "foregroundImage": "./assets/images/icons/adaptive-icon.png",
              "backgroundColor": "#ffffff"
            }
          }
        }
      ],
      "@react-native-google-signin/google-signin",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "expo-font",
      "expo-web-browser",
      "expo-quick-actions",
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b055ed93-055e-4bb6-a5c5-453e7f1ed80e"
      }
    }
  }
}